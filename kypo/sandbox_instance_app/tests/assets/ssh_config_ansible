    HostName 201.201.2.133
    User debian
    IdentityFile /root/.ssh/pool_mng_key
    AddKeysToAgent yes
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    ProxyJump debian@jump-host-ip

