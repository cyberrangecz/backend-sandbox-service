Host jump-host-ip
    HostName jump-host-ip
    User pool-prefix
    IdentityFile <path_to_pool_private_key>
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    IdentitiesOnly yes

Host man 10.10.10.10
    HostName 10.10.10.10
    User debian
    IdentityFile <path_to_pool_private_key>
    ProxyJump pool-prefix@jump-host-ip
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    IdentitiesOnly yes

